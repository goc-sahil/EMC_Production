       <apex:column value="{!Item.Month__c}"/>
       <apex:column value="{!Item.MPG__c}"/>
       <apex:column value="{!Item.Monthly_Mileage__c}"/>
       <apex:column value="{!Item.Fuel_Price__c}"/>
