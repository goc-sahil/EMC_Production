       <apex:column value="{!Item.Fixed_Amount__c}"/>
       <apex:column value="{!Item.Total_Variable_Rate__c}"/>
       <apex:column value="{!Item.Monthly_Variable_Amount__c}"/>
  
  </apex:pageBlockTable>
  </apex:pageBlock>
