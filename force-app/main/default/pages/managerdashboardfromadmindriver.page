                            $scope.loadpage();
                        }
                    },
                    { escape: false }
                );
            }


            $scope.viewchartdata = new viewChartDialogModel(false);
            $scope.viewchart = function () {

                $scope.data = [];
                $scope.data = JSON.parse('{!ChartData}');
                $scope.viewchartdata.open($scope.data);
            }
            $('#spinner').hide();
    }]);
        </script>
