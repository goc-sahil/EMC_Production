            function removeDumplicateValue(myArray) {
                var newArray = [];
                angular.forEach(myArray, function (value, key) {
                    var exists = false;
                    angular.forEach(newArray, function (val2, key) {
                        if (angular.equals(value, val2)) { exists = true };
                    });
                    if (exists == false && value != "") {
                        newArray.push(value);
                    }
                });
                return newArray;
            }


